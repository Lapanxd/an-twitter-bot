{
  "name": "an-twitter-bot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only --clear src/main.ts",
    "format": "prettier --write \"src/**/*.{ts,js,json,md,html}\"",
    "postinstall": "npm run build",
    "prepare": "husky install",
    "migration:generate": "ts-node --esm ./node_modules/typeorm/cli.js migration:generate -d src/data-source.ts src/migrations/migration",
    "migration:run": "ts-node --esm ./node_modules/typeorm/cli.js migration:run -d src/data-source.ts",
    "migration:revert": "ts-node --esm ./node_modules/typeorm/cli.js migration:revert -d src/data-source.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.4.0",
    "node-cron": "^4.1.1",
    "openai": "^5.5.1",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "twitter-api-v2": "^1.23.2",
    "typescript": "^5.8.3",
    "unzipper": "^0.12.3",
    "winston": "^3.17.0",
    "xml2js": "^0.6.2",
    "typeorm": "^0.3.24",
    "@types/node": "^24.0.3",
    "@types/unzipper": "^0.10.11",
    "@types/winston": "^2.4.4",
    "@types/xml2js": "^0.4.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "husky": "^8.0.0",
    "ts-node-dev": "^2.0.0"
  }
}
